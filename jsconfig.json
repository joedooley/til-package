{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["./src/components/*"],
      "@context/*": ["./src/context/*"],
      "@hooks/*": ["./src/hooks/*"],
      "@util/*": ["./src/util/*"],
      "@styles/*": ["./src/styles/*"],
      "@test/*": ["src/test/*"],
      "@lib/*": ["src/lib/*"]
